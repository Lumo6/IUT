n=1:1:4;
Un1=2.*n+sin(n);
Un2=1:1:4
Un2(1)=1+1/2-1
Un2(2)=1+1/2+1/4-2
Un2(3)=1+1/2+1/4+1/8-3
Un2(4)=1+1/2+1/4+1/8+1/16-4
Un3=exp(n)./n;
Un4=1:1:4
Un4(1)=1/2;
Un4(2)=(1/2)*(3/4);
Un4(3)=(1/2)*(3/4)*(5/6);
Un4(4)=(1/2)*(3/4)*(5/6)*(7/8);
Un5=sqrt(n)-3./sqrt(n);


plot(n,Un1,'Or')
plot(n,Un2,'Ob')
plot(n,Un3,'Oo')
plot(n,Un4,'Ob')
plot(n,Un5,'Oy')
